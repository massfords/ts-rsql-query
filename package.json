  "homepage": "https://github.com/massfords/ts-rsql-query#readme",
  "devDependencies": {
    "@types/common-tags": "^1.8.4",
    "@types/jest": "^29.5.14",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^8.57.1",
