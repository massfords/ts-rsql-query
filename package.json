    "pg-promise": "^11.10.1",
    "prettier": "^3.3.3",
    "testcontainers": "^9.1.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "uuid": "^11.0.0"
